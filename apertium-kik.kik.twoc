"Match reflexive, imperative, and negative part 1"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  ( %[%-refl%] %[%-imp%] )
         Ty in ( %[%+refl%] %[%+imp%] )
   matched ;

"Match reflexive, imperative, and negative part 1.5"
Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Ty in  ( %[%-refl%] %[%-imp%] )
         Tx in ( %[%+refl%] %[%+imp%] )
   matched ;

 "Match reflexive, imperative, and negative part 2"
 Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  ( %[%-neg%] )
         Ty in ( %[%+neg%] )
   matched ;

 "Match reflexive, imperative, and negative part 2"
 Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Tx in  ( %[%+neg%] )
         Ty in ( %[%-neg%] )
   matched ;


 "Match aspect part 1"
 Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Tx in  ( %[%+compl%] %[%+hab%] )
         Ty in ( %[%-compl%] %[%-hab%] )
