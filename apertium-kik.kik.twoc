Rules

"Match number prefixes and person"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-sg%]  )
         Ty in ( %[%+sg%] )
   matched ;

"Match number prefixes and person 2"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-pl%] )
         Ty in ( %[%+pl%] )
   matched ;


"Match number prefixes and person 2"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-p1%] %[%-p2%] )
         Ty in ( %[%+p1%] %[%+p2%] )
   matched ;

"Match tense and focus, part 1a"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-currpast%] %[%-pres%] )
         Ty in ( %[%+currpast%] %[%+pres%] )
   matched ;

"Match tense and focus, part 1b"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-currfut%] %[%-remfut%] )
         Ty in ( %[%+currfut%] %[%+remfut%] )
   matched ;

"Match tense and focus, part 2a"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-foc%] %[%-seq%] )
         Ty in ( %[%+foc%] %[%+seq%] )
   matched ;

 "Match tense and focus, part 2a.5"
 Ty:0 /<= _ ;
   except
       Tx:0 (:*) _ ;
   where Tx in ( %[%-foc%] %[%-seq%] )
         Ty in ( %[%+foc%] %[%+seq%] )
   matched ;

"Match tense and focus, part 2b"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in ( %[%-rempast%] %[%-nearpast%] )
         Ty in ( %[%+rempast%] %[%+nearpast%] )
   matched ;

"Match noun class and person, part 1a"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  ( %[%-cl1%] %[%-cl3%] )
         Ty in ( %[%+cl1%] %[%+cl3%] )
   matched ;

"Match noun class and person, part 2a"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  (  %[%-cl9p6%] %[%-cl9p1%0%] )
         Ty in ( %[%+cl9p6%] %[%+cl9p1%0%] )
   matched ;

"Match noun class and person, part 3a"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  ( %[%-cl12p6%] %[%-cl12p13%] )
         Ty in ( %[%+cl12p6%] %[%+cl12p13%] )
   matched ;

"Match noun class and person, part 1b"
Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
   where Tx in  ( %[%-cl5%] %[%-cl7%] )
         Ty in ( %[%+cl5%] %[%+cl7%] )
