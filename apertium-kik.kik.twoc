 "Match aspect part 2"
 Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Tx in  ( %[%+noasp%] %[%+perf%] )
         Ty in ( %[%-noasp%] %[%-perf%] )
   matched ;

 "Match aspect part 3"
 Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Tx in  ( %[%+proc%] )
         Ty in ( %[%-proc%] )
   matched ;

 "Coordinate final vowels"
 Tx:0 /<= _ ;
   except
       Ty:0 (:*) _ ;
   where Tx in  ( %[%+fvA%] %[%+fvE%] )
         Ty in ( %[%-fvA%] %[%-fvE%] )
   matched ;


 "Coordinate final vowels (reverse) "
 Tx:0 /<= _ ;
   except
       _ (:*) Ty:0 ;
!       _ (:*) %[%+fvĨ%]:0 ;
   where Ty in  ( %[%+fvA%] %[%+fvE%]  )
         Tx in ( %[%-fvA%] %[%-fvE%] )
   matched ;

